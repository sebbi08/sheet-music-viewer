{
  "name": "sheet-music-viewer",
  "productName": "sheet-music-viewer",
  "version": "1.8.0",
  "description": "App to display music sheets",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package --arch x64",
    "make": "electron-forge make --arch x64",
    "publish": "electron-forge publish --arch x64",
    "semantic-release": "semantic-release --no-ci",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "electron-forge publish --arch x64 --from-dry-run",
    "release": "yarn semantic-release --no-ci && git push && npm run publish"
  },
  "keywords": [],
  "author": {
    "name": "Sebastian Mahr",
    "email": "sebastian.mahr83@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "7.8.0",
    "@electron-forge/maker-deb": "7.8.0",
    "@electron-forge/maker-rpm": "7.8.0",
    "@electron-forge/maker-squirrel": "7.8.0",
    "@electron-forge/maker-zip": "7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "7.8.0",
    "@electron-forge/plugin-fuses": "7.8.0",
    "@electron-forge/plugin-vite": "7.8.0",
    "@electron/fuses": "1.8.0",
    "@eslint/compat": "^1.2.8",
    "@mdi/font": "7.4.47",
    "@tsconfig/node22": "22.0.1",
    "@types/debug": "4.1.12",
    "@types/electron-devtools-installer": "4.0.0",
    "@types/electron-squirrel-startup": "1.0.2",
    "@types/fontfaceobserver": "2.1.3",
    "@types/fs-extra": "11.0.4",
    "@types/lodash": "4.17.16",
    "@types/node": "22.14.0",
    "@types/node-fetch": "2.6.12",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/tsconfig": "0.7.0",
    "core-js": "3.41.0",
    "debug": "4.4.0",
    "electron": "35.1.4",
    "electron-devtools-installer": "4.0.0",
    "electron-log": "5.3.3",
    "electron-squirrel-startup": "1.0.1",
    "eslint": "9.24.0",
    "eslint-plugin-import": "2.31.0",
    "fabric": "6.6.2",
    "form-data": "4.0.2",
    "fs-extra": "11.3.0",
    "glob": "11.0.1",
    "less": "4.3.0",
    "lodash": "4.17.21",
    "node-fetch": "3.3.2",
    "pdfjs-dist": "5.1.91",
    "pinia": "^3.0.1",
    "roboto-fontface": "0.10.0",
    "sass": "1.86.3",
    "sass-embedded": "1.86.3",
    "semantic-release": "24.2.3",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "unplugin-fonts": "1.3.1",
    "unplugin-vue-components": "28.4.1",
    "vite": "5.4.17",
    "vite-plugin-vuetify": "2.1.1",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue-tsc": "2.2.8",
    "vuedraggable": "4.1.0",
    "vuetify": "3.8.0"
  },
  "dependencies": {
    "@electron-forge/maker-flatpak": "7.8.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.3",
    "electron-devtools-installer": "4.0.0",
    "fontfaceobserver": "2.3.0",
    "material-design-icons": "3.0.1",
    "typeface-roboto": "1.1.13"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
