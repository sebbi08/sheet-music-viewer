{
  "name": "sheet-music-viewer",
  "productName": "sheet-music-viewer",
  "version": "1.6.1",
  "description": "App to display music sheets",
  "main": ".webpack/main",
  "type": "commonjs",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package --arch x64",
    "make": "electron-forge make --arch x64",
    "publish": "electron-forge publish --arch x64",
    "semantic-release": "semantic-release --no-ci",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "electron-forge publish --arch x64 --from-dry-run",
    "release": "rimraf out .webpack && yarn semantic-release --no-ci && git push && npm run publish"
  },
  "keywords": [],
  "author": {
    "name": "Sebastian Mahr",
    "email": "sebastian.mahr83@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-webpack": "^6.4.2",
    "@electron-forge/publisher-electron-release-server": "^6.4.2",
    "@mdi/font": "6.3.95",
    "@types/debug": "^4.1.7",
    "@types/electron-devtools-installer": "^2.2.4",
    "@types/fabric": "^4.5.3",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.175",
    "@types/node-fetch": "^2.6.2",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "axios": "^0.23.0",
    "core-js": "^3.6.5",
    "css-loader": "^6.7.2",
    "debug": "^4.3.4",
    "deepmerge": "^4.2.2",
    "electron": "27.0.3",
    "electron-log": "^5.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "fabric": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "node-loader": "^2.0.0",
    "pdfjs-dist": "^2.13.216",
    "roboto-fontface": "*",
    "sass": "~1.32",
    "sass-loader": "^10.4.1",
    "semantic-release": "^22.0.7",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.5.4",
    "vue": "2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-loader": "^15.10.1",
    "vue-pdf-app": "^2.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vue-template-compiler": "2.6.14",
    "vuedraggable": "^2.24.3",
    "vuetify": "2.5.7",
    "vuetify-loader": "1.7.2",
    "vuex": "^3.4.0",
    "vuex-map-fields": "^1.4.1",
    "worker-loader": "^3.0.8",
    "worker-plugin": "^5.0.1"
  },
  "dependencies": {
    "@electron-forge/maker-flatpak": "^6.4.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/worker-plugin": "^5.0.1",
    "dotenv": "^16.0.3",
    "electron-devtools-installer": "^3.2.0",
    "material-design-icons": "^3.0.1",
    "typeface-roboto": "^1.1.13",
    "url-loader": "^4.1.1"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
